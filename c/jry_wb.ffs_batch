            <Item>\jry_wb\jry_wb_netdisk\jry_nd.fast_save_message</Item>
            <Item>\jry_wb\jry_wb_tp_sdk\</Item>
        </Exclude>
        <TimeSpan Type="None">0</TimeSpan>
        <SizeMin Unit="None">0</SizeMin>
        <SizeMax Unit="None">0</SizeMax>
    </Filter>
    <FolderPairs>
        <Pair>
            <Left>D:\mydocument\science\juruoyun\juruoyun_bl\c\jry_bl</Left>
            <Right>D:\mydocument\science\juruoyun\juruoyun_wl\c\jry_bl</Right>
        </Pair>
    </FolderPairs>
    <Errors Ignore="true" Retry="0" Delay="5"/>
    <LogFolder/>
    <PostSyncCommand Condition="Completion"/>
