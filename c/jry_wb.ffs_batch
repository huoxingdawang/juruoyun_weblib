    <Filter>
        <Include>
            <Item>*</Item>
        </Include>
        <Exclude>
            <Item>\System Volume Information\</Item>
            <Item>\$Recycle.Bin\</Item>
            <Item>\RECYCLER\</Item>
            <Item>\RECYCLED\</Item>
            <Item>*\desktop.ini</Item>
            <Item>*\thumbs.db</Item>
            <Item>\logreport\</Item>
            <Item>\.git\</Item>
            <Item>*\_notes\</Item>
            <Item>\.gitignore</Item>
            <Item>\data\</Item>
            <Item>\index2.php</Item>
